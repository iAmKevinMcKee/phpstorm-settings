  <template name="datatablesjs" value="$(document).ready(function() {&#10;            // Setup - add a text input to each footer cell&#10;            $('#$tablename$ tfoot th').each( function () {&#10;                var title = $(this).text();&#10;                $(this).html( '&lt;input type=&quot;text&quot; placeholder=&quot;Search&quot; /&gt;' );&#10;            } );&#10;&#10;            // DataTable&#10;            var table = $('#$tablename$').DataTable();&#10;&#10;            // Apply the search&#10;            table.columns().every( function () {&#10;                var that = this;&#10;&#10;                $( 'input', this.footer() ).on( 'keyup change', function () {&#10;                    if ( that.search() !== this.value ) {&#10;                        that&#10;                            .search( this.value )&#10;                            .draw();&#10;                    }&#10;                } );&#10;            } );&#10;&#10;            $('#$tablename$ tfoot tr').appendTo('#$tablename$ thead');&#10;&#10;        } );" description="Basic Datatables Javascript" toReformat="true" toShortenFQNames="true">
